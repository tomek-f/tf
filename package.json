{
    "name": "tf",
    "private": true,
    "type": "module",
    "engines": {
        "node": "^18 || ^20"
    },
    "packageManager": "pnpm@8.15.1",
    "scripts": {
        "turbo:build": "turbo build",
        "turbo:check-types": "turbo check-types --force",
        "turbo:lint-js": "turbo lint-js --force",
        "turbo:lint-js-fix": "turbo lint-js-fix --force",
        "turbo:lint-js-next": "turbo lint-js-next --force",
        "turbo:test": "turbo test --force",
        "turbo:astro-check": "turbo astro-check --force --concurrency=1",
        "turbo:astro-sync": "turbo astro-sync --force --concurrency=1",
        "turbo:build-slash-run-pocs": "turbo build run --force --filter='./pocs/*' --filter=!@tf/astro-ssr --filter=!@tf/next-playground",
        "format-check": "prettier --check . **/*.astro",
        "format-write": "prettier --write . **/*.astro",
        "beforecommitTEMP": "NODE_ENV=production lint-staged && pnpm run turbo:astro-check && turbo check-types test --force",
        "beforecommit": "NODE_ENV=production lint-staged && pnpm run turbo:astro-sync && turbo check-types test --force",
        "prettier-permission-fix": "chmod +x ./node_modules/.bin/prettier",
        "vercel": "vercel",
        "turbo": "turbo"
    },
    "devDependencies": {
        "@ianvs/prettier-plugin-sort-imports": "^4.1.1",
        "@total-typescript/ts-reset": "^0.5.1",
        "@types/node": "^20.11.16",
        "@typescript-eslint/eslint-plugin": "^6.20.0",
        "@typescript-eslint/parser": "^6.20.0",
        "eslint": "^8.56.0",
        "eslint-config-prettier": "^9.1.0",
        "eslint-import-resolver-typescript": "^3.6.1",
        "eslint-plugin-import": "^2.29.1",
        "eslint-plugin-prettier": "^5.1.3",
        "eslint-plugin-react": "^7.33.2",
        "eslint-plugin-react-hooks": "5.0.0-canary-7118f5dd7-20230705",
        "eslint-plugin-react-refresh": "^0.4.5",
        "eslint-plugin-typescript-sort-keys": "^3.1.0",
        "eslint-plugin-unicorn": "^50.0.1",
        "eslint-plugin-workspaces": "^0.10.0",
        "lint-staged": "^15.2.1",
        "pre-commit": "^1.2.2",
        "prettier": "^3.2.4",
        "prettier-plugin-astro": "^0.13.0",
        "prettier-plugin-tailwindcss": "^0.5.11",
        "turbo": "^1.12.2",
        "typescript": "^5.3.3"
    },
    "pre-commit": "beforecommit"
}
