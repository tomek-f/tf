{
    "name": "@tf/run-script",
    "version": "0.3.0",
    "private": true,
    "type": "module",
    "engines": {
        "node": "^18 || ^20"
    },
    "main": "./out/index.js",
    "bin": {
        "ntl": "./out/index.js",
        "run-script": "./out/index.js",
        "tfrs": "./out/index.js",
        "rrr": "./out/index.js"
    },
    "files": [
        "out"
    ],
    "scripts": {
        "start": "node ./out/index.js",
        "build": "tsc --noEmit false &&  mv ./out/index.js ./out/index-temp.js && cat ./src/prepend.txt ./out/index-temp.js > out/index.js && rm ./out/index-temp.js",
        "test1": "echo 'test 1'",
        "test2": "echo 'test 2'",
        "lint-js": "NODE_ENV=production eslint --ext .js,.jsx,.cjs,.mjs,.ts,.tsx ./",
        "lint-js-fix": "pnpm run lint-js --fix",
        "check-types": "tsc --skipLibCheck true -p ./tsconfig.json"
    },
    "dependencies": {
        "detect-package-manager": "^3.0.1",
        "gradient-string": "^2.0.2",
        "inquirer": "^9.2.13"
    },
    "devDependencies": {
        "@types/gradient-string": "^1.1.5",
        "@types/inquirer": "^9.0.7",
        "@types/node": "^20.11.16",
        "eslint": "^8.56.0",
        "typescript": "^5.3.3"
    }
}
