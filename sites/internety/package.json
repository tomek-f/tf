{
    "name": "@tf/internety",
    "version": "0.0.0",
    "private": true,
    "type": "module",
    "engines": {
        "node": "^20 || ^21 || ^22"
    },
    "scripts": {
        "dev": "rimraf ./.next && next dev",
        "dev-turbopack": "rimraf ./.next && next dev --turbo",
        "build": "rimraf ./.next && next build",
        "build-and-preview": "pnpm run build && pnpm run preview",
        "preview": "next start",
        "start": "next start",
        "lint-js-next": "next lint",
        "lint-js": "NODE_ENV=production eslint",
        "lint-js-fix": "pnpm run lint-js --fix",
        "check-types": "tsc --skipLibCheck true -p ./tsconfig.json && echo 'no problems'",
        "next": "next"
    },
    "dependencies": {
        "@hookform/resolvers": "^3.9.0",
        "@libsql/client": "^0.7.0",
        "@reduxjs/toolkit": "^2.2.6",
        "clsx": "^2.1.1",
        "lucide-react": "^0.414.0",
        "next": "^14.2.5",
        "react": "^18.3.1",
        "react-dom": "^18.3.1",
        "react-hook-form": "^7.52.1",
        "sharp": "^0.33.4",
        "zod": "^3.23.8"
    },
    "devDependencies": {
        "@types/node": "^20.14.11",
        "@types/react": "^18.3.3",
        "@types/react-dom": "^18.3.0",
        "autoprefixer": "^10.4.19",
        "eslint": "^9.7.0",
        "eslint-config-next": "14.2.5",
        "postcss": "^8.4.39",
        "rimraf": "^6.0.1",
        "tailwindcss": "^3.4.6",
        "typescript": "^5.5.3"
    },
    "postcss": {
        "plugins": {
            "tailwindcss": {},
            "autoprefixer": {}
        }
    }
}
