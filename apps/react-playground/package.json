{
  "name": "@tf/react-playground",
  "version": "1.0.0",
  "author": "Tomek Fijo≈Ç <tomekfijol@gmail.com> (http://tomekf.pl/)",
  "private": true,
  "type": "module",
  "engines": {
    "node": ">=16.0.0 <19.0.0"
  },
  "scripts": {
    "dev": "tsc -p tsconfig.dev.json && vite",
    "dev-org": "vite",
    "build-and-serve": "npm run build && npm run serve",
    "build": "tsc && vite build",
    "serve": "wds --root-dir dist --watch --open --app-index dist/index.html --port 4173",
    "server-org": "vite preview",
    "lint-js": "NODE_ENV=production eslint --ext .js,.jsx,.cjs,.mjs,.ts,.tsx ./",
    "lint-js-fix": "npm run lint-js --fix",
    "check-types": "tsc --skipLibCheck true -p ./tsconfig.json && echo 'no problems'",
    "test:watch": "vitest --reporter=verbose",
    "test:ui": "vitest --ui",
    "test": "vitest --reporter=verbose run"
  },
  "dependencies": {
    "@reduxjs/toolkit": "^1.9.3",
    "classnames": "^2.3.2",
    "history": "^5.3.0",
    "immer": "^9.0.21",
    "jotai": "^2.0.4",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-helmet-async": "^1.3.0",
    "react-intl": "^6.3.2",
    "react-redux": "^8.0.5",
    "react-router": "^6.10.0",
    "react-router-dom": "^6.10.0",
    "scheduler": "^0.23.0",
    "use-context-selector": "^1.4.1",
    "use-immer": "^0.9.0",
    "zustand": "^4.3.7"
  },
  "devDependencies": {
    "@types/react": "^18.0.35",
    "@types/react-dom": "^18.0.11",
    "@vitejs/plugin-react-swc": "^3.3.0",
    "@vitest/ui": "^0.30.1",
    "@web/dev-server": "^0.1.38",
    "autoprefixer": "^10.4.14",
    "eslint": "^8.38.0",
    "happy-dom": "^9.3.2",
    "postcss": "^8.4.21",
    "rollup-plugin-visualizer": "^5.9.0",
    "tailwindcss": "^3.3.1",
    "typescript": "^5.0.4",
    "vite": "^4.2.1",
    "vitest": "^0.30.1"
  },
  "homepage": "./"
}
