{
  "name": "@tf/react-playground",
  "version": "1.0.0",
  "author": "Tomek Fijo≈Ç <tomekfijol@gmail.com> (http://tomekf.pl/)",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "tsc -p tsconfig.dev.json && vite",
    "dev-org": "vite",
    "build-and-serve": "npm run build && npm run serve",
    "build": "tsc && vite build",
    "serve": "wds --root-dir dist --watch --open --app-index dist/index.html --port 4173",
    "server-org": "vite preview",
    "lint-js": "NODE_ENV=production eslint --ext .js,.jsx,.cjs,.mjs,.ts,.tsx ./",
    "lint-js-fix": "npm run lint-js --fix",
    "check-types": "tsc --skipLibCheck true -p ./tsconfig.json && echo 'no problems'",
    "test:watch": "vitest --reporter=verbose",
    "test:ui": "vitest --ui",
    "test": "vitest --reporter=verbose run"
  },
  "dependencies": {
    "@reduxjs/toolkit": "^1.9.5",
    "clsx": "^1.2.1",
    "history": "^5.3.0",
    "immer": "^10.0.2",
    "jotai": "^2.1.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-helmet-async": "^1.3.0",
    "react-intl": "^6.4.2",
    "react-redux": "^8.0.5",
    "react-router": "^6.11.1",
    "react-router-dom": "^6.11.1",
    "scheduler": "^0.23.0",
    "use-context-selector": "^1.4.1",
    "use-immer": "^0.9.0",
    "zustand": "^4.3.8"
  },
  "devDependencies": {
    "@types/react": "^18.2.6",
    "@types/react-dom": "^18.2.4",
    "@vitejs/plugin-react-swc": "^3.3.1",
    "@vitest/ui": "^0.31.0",
    "@web/dev-server": "^0.2.1",
    "autoprefixer": "^10.4.14",
    "eslint": "^8.40.0",
    "happy-dom": "^9.18.3",
    "postcss": "^8.4.23",
    "rollup-plugin-visualizer": "^5.9.0",
    "tailwindcss": "^3.3.2",
    "typescript": "^5.0.4",
    "vite": "^4.3.6",
    "vitest": "^0.31.0"
  },
  "homepage": "./"
}
