{
    "name": "@tf/astro-ssr",
    "version": "1.1.0",
    "private": true,
    "type": "module",
    "engines": {
        "node": "^18 || ^20"
    },
    "scripts": {
        "dev": "astro dev",
        "start": "astro dev",
        "build-and-preview": "npm run build && npm run preview",
        "build": "astro build",
        "preview": "astro preview",
        "preview-node": "node ./dist/server/entry.mjs",
        "preview-node-custom": "HOST=0.0.0.0 PORT=4321 node ./dist/server/entry.mjs",
        "check": "astro check",
        "sync": "astro sync",
        "astro": "astro",
        "lint-js": "NODE_ENV=production eslint --ext .js,.jsx,.cjs,.mjs,.ts,.tsx ./",
        "lint-js-fix": "npm run lint-js --fix",
        "check-types": "astro check && tsc --skipLibCheck true -p ./tsconfig.json && echo 'no problems'"
    },
    "devDependencies": {
        "@types/runes": "^0.4.1",
        "eslint": "^8.50.0",
        "tailwindcss": "^3.3.3",
        "type-fest": "^4.3.3",
        "typescript": "^5.2.2"
    },
    "dependencies": {
        "@astrojs/check": "^0.2.0",
        "@astrojs/node": "^6.0.3",
        "@astrojs/solid-js": "^3.0.2",
        "@astrojs/tailwind": "^5.0.1",
        "astro": "^3.2.3",
        "lucide-solid": "^0.284.0",
        "runes": "^0.4.3",
        "sharp": "^0.32.6",
        "solid-js": "^1.7.12",
        "tailwind-merge": "^1.14.0"
    }
}
