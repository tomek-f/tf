{
    "name": "@tf/react-playground",
    "version": "1.1.0",
    "private": true,
    "type": "module",
    "engines": {
        "node": "^20"
    },
    "scripts": {
        "dev": "vite",
        "dev-dev": "tsc -p tsconfig.dev.json && vite",
        "build-and-preview": "npm run build && npm run preview",
        "build": "tsc && vite build",
        "preview": "vite preview",
        "_ntl_hack_preview": "vite preview",
        "lint-js": "NODE_ENV=production eslint --ext .js,.jsx,.cjs,.mjs,.ts,.tsx ./",
        "lint-js-fix": "npm run lint-js --fix",
        "check-types": "tsc --skipLibCheck true -p ./tsconfig.json && echo 'no problems'",
        "test:watch": "vitest --reporter=verbose",
        "test:ui": "vitest --ui",
        "test": "vitest --reporter=verbose run"
    },
    "dependencies": {
        "@reduxjs/toolkit": "^2.0.1",
        "clsx": "^2.0.0",
        "history": "^5.3.0",
        "immer": "^10.0.3",
        "jotai": "^2.6.0",
        "react": "^18.2.0",
        "react-dom": "^18.2.0",
        "react-helmet-async": "^2.0.4",
        "react-intl": "^6.5.5",
        "react-redux": "^9.0.4",
        "react-router": "^6.21.0",
        "react-router-dom": "^6.21.0",
        "scheduler": "^0.23.0",
        "use-context-selector": "^1.4.1",
        "use-immer": "^0.9.0",
        "zustand": "^4.4.7"
    },
    "devDependencies": {
        "@types/react": "^18.2.45",
        "@types/react-dom": "^18.2.18",
        "@vitejs/plugin-react-swc": "^3.5.0",
        "@vitest/ui": "^1.1.0",
        "autoprefixer": "^10.4.16",
        "eslint": "^8.56.0",
        "happy-dom": "^12.10.3",
        "postcss": "^8.4.32",
        "rollup-plugin-visualizer": "^5.11.0",
        "tailwindcss": "^3.4.0",
        "typescript": "^5.3.3",
        "vite": "^5.0.10",
        "vitest": "^1.1.0"
    },
    "postcss": {
        "plugins": {
            "tailwindcss": {},
            "autoprefixer": {}
        }
    }
}
