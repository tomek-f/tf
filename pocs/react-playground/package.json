{
    "name": "@tf/react-playground",
    "version": "1.1.0",
    "private": true,
    "type": "module",
    "engines": {
        "node": "^18 || ^20"
    },
    "scripts": {
        "dev": "vite",
        "dev-dev": "tsc -p tsconfig.dev.json && vite",
        "build-and-preview": "npm run build && npm run preview",
        "build": "tsc && vite build",
        "preview": "vite preview",
        "_ntl_hack_preview": "vite preview",
        "lint-js": "NODE_ENV=production eslint --ext .js,.jsx,.cjs,.mjs,.ts,.tsx ./",
        "lint-js-fix": "npm run lint-js --fix",
        "check-types": "tsc --skipLibCheck true -p ./tsconfig.json && echo 'no problems'",
        "test:watch": "vitest --reporter=verbose",
        "test:ui": "vitest --ui",
        "test": "vitest --reporter=verbose run"
    },
    "dependencies": {
        "@reduxjs/toolkit": "^1.9.5",
        "clsx": "^2.0.0",
        "history": "^5.3.0",
        "immer": "^10.0.2",
        "jotai": "^2.4.2",
        "react": "^18.2.0",
        "react-dom": "^18.2.0",
        "react-helmet-async": "^1.3.0",
        "react-intl": "^6.4.6",
        "react-redux": "^8.1.2",
        "react-router": "^6.15.0",
        "react-router-dom": "^6.15.0",
        "scheduler": "^0.23.0",
        "use-context-selector": "^1.4.1",
        "use-immer": "^0.9.0",
        "zustand": "^4.4.1"
    },
    "devDependencies": {
        "@types/react": "^18.2.21",
        "@types/react-dom": "^18.2.7",
        "@vitejs/plugin-react-swc": "^3.3.2",
        "@vitest/ui": "^0.34.4",
        "autoprefixer": "^10.4.15",
        "eslint": "^8.49.0",
        "happy-dom": "^11.0.5",
        "postcss": "^8.4.29",
        "rollup-plugin-visualizer": "^5.9.2",
        "tailwindcss": "^3.3.3",
        "typescript": "^5.2.2",
        "vite": "^4.4.9",
        "vitest": "^0.34.4"
    }
}
