{
  "name": "@tf/react-playground",
  "version": "1.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "tsc -p tsconfig.dev.json && vite",
    "dev-org": "vite",
    "build-and-preview": "npm run build && npm run preview",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "_ntl_hack_preview": "vite preview",
    "lint-js": "NODE_ENV=production eslint --ext .js,.jsx,.cjs,.mjs,.ts,.tsx ./",
    "lint-js-fix": "npm run lint-js --fix",
    "check-types": "tsc --skipLibCheck true -p ./tsconfig.json && echo 'no problems'",
    "test:watch": "vitest --reporter=verbose",
    "test:ui": "vitest --ui",
    "test": "vitest --reporter=verbose run"
  },
  "dependencies": {
    "@reduxjs/toolkit": "^1.9.5",
    "clsx": "^1.2.1",
    "history": "^5.3.0",
    "immer": "^10.0.2",
    "jotai": "^2.2.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-helmet-async": "^1.3.0",
    "react-intl": "^6.4.4",
    "react-redux": "^8.1.1",
    "react-router": "^6.13.0",
    "react-router-dom": "^6.13.0",
    "scheduler": "^0.23.0",
    "use-context-selector": "^1.4.1",
    "use-immer": "^0.9.0",
    "zustand": "^4.3.8"
  },
  "devDependencies": {
    "@types/react": "^18.2.13",
    "@types/react-dom": "^18.2.6",
    "@vitejs/plugin-react-swc": "^3.3.2",
    "@vitest/ui": "^0.32.2",
    "autoprefixer": "^10.4.14",
    "eslint": "^8.43.0",
    "happy-dom": "^9.20.3",
    "postcss": "^8.4.24",
    "rollup-plugin-visualizer": "^5.9.2",
    "tailwindcss": "^3.3.2",
    "typescript": "^5.1.3",
    "vite": "^4.3.9",
    "vitest": "^0.32.2"
  }
}
