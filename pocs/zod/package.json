{
  "name": "@tf/zod",
  "version": "1.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "tsc -p tsconfig.dev.json && vite",
    "dev-org": "vite",
    "build-and-preview": "npm run build && npm run preview",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "_ntl_hack_preview": "vite preview",
    "lint-js": "NODE_ENV=production eslint --ext .js,.jsx,.cjs,.mjs,.ts,.tsx ./ --report-unused-disable-directives --max-warnings 0",
    "lint-js-fix": "npm run lint-js --fix",
    "check-types": "tsc --skipLibCheck true -p ./tsconfig.json && echo 'no problems'"
  },
  "dependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "zod": "^3.21.4"
  },
  "devDependencies": {
    "@types/react": "^18.0.37",
    "@types/react-dom": "^18.0.11",
    "@vitejs/plugin-react-swc": "^3.0.0",
    "eslint": "^8.38.0",
    "eslint-plugin-react-refresh": "^0.3.4",
    "typescript": "^5.0.2",
    "vite": "^4.3.9"
  }
}
