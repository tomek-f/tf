{
    "extends": "../../tsconfig.json",
    "compilerOptions": {
        "skipLibCheck": true,
        "jsx": "preserve",
        "plugins": [{ "name": "next" }],
        "strictNullChecks": true,
        "baseUrl": ".",
        "paths": { "T3/*": ["./src/*"] }
    },
    "include": [
        "../../@types",
        ".eslintrc.cjs",
        "next-env.d.ts",
        "**/*.ts",
        "**/*.tsx",
        "**/*.cjs",
        "**/*.mjs",
        ".next/types/**/*.ts"
    ],
    "exclude": ["node_modules"]
}
