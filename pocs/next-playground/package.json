{
    "name": "@tf/next-playground",
    "version": "1.1.0",
    "private": true,
    "type": "module",
    "engines": {
        "node": "^20"
    },
    "scripts": {
        "dev": "rimraf ./.next && next dev",
        "dev-turbo": "rimraf ./.next && next dev --turbo",
        "build": "rimraf ./.next && next build",
        "build-and-preview": "npm run build && npm run preview",
        "preview": "next start",
        "start": "next start",
        "lint-js-next": "next lint",
        "lint-js": "NODE_ENV=production eslint --ext .js,.jsx,.cjs,.mjs,.ts,.tsx ./",
        "lint-js-fix": "npm run lint-js --fix",
        "check-types": "tsc --skipLibCheck true -p ./tsconfig.json && echo 'no problems'",
        "next": "next"
    },
    "dependencies": {
        "@hookform/resolvers": "^3.3.2",
        "@libsql/client": "^0.3.6",
        "@reduxjs/toolkit": "^2.0.1",
        "clsx": "^2.0.0",
        "lucide-react": "^0.301.0",
        "next": "14.0.4",
        "react": "^18.2.0",
        "react-dom": "^18.2.0",
        "react-hook-form": "^7.49.2",
        "zod": "^3.22.4"
    },
    "devDependencies": {
        "@types/node": "^20.10.5",
        "@types/react": "^18.2.45",
        "@types/react-dom": "^18.2.18",
        "autoprefixer": "^10.4.16",
        "eslint": "^8.56.0",
        "eslint-config-next": "14.0.4",
        "postcss": "^8.4.32",
        "tailwindcss": "^3.4.0",
        "typescript": "^5.3.3"
    },
    "postcss": {
        "plugins": {
            "tailwindcss": {},
            "autoprefixer": {}
        }
    }
}
