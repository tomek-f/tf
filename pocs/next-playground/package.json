{
    "name": "@tf/next-playground",
    "version": "0.0.0",
    "private": true,
    "type": "module",
    "engines": {
        "node": "^18 || ^20"
    },
    "scripts": {
        "dev": "rimraf ./.next && next dev",
        "dev-turbo": "rimraf ./.next && next dev --turbo",
        "build": "rimraf ./.next && next build",
        "build-and-preview": "pnpm run build && pnpm run preview",
        "preview": "next start",
        "start": "next start",
        "lint-js-next": "next lint",
        "lint-js": "NODE_ENV=production eslint --ext .js,.jsx,.cjs,.mjs,.ts,.tsx ./",
        "lint-js-fix": "pnpm run lint-js --fix",
        "check-types": "tsc --skipLibCheck true -p ./tsconfig.json && echo 'no problems'",
        "next": "next"
    },
    "dependencies": {
        "@hookform/resolvers": "^3.3.4",
        "@libsql/client": "^0.4.3",
        "@reduxjs/toolkit": "^2.1.0",
        "clsx": "^2.1.0",
        "lucide-react": "^0.321.0",
        "next": "14.1.0",
        "react": "^18.2.0",
        "react-dom": "^18.2.0",
        "react-hook-form": "^7.50.0",
        "sharp": "^0.33.2",
        "zod": "^3.22.4"
    },
    "devDependencies": {
        "@types/node": "^20.11.16",
        "@types/react": "^18.2.55",
        "@types/react-dom": "^18.2.18",
        "autoprefixer": "^10.4.17",
        "eslint": "^8.56.0",
        "eslint-config-next": "14.1.0",
        "postcss": "^8.4.34",
        "rimraf": "^5.0.5",
        "tailwindcss": "^3.4.1",
        "typescript": "^5.3.3"
    },
    "postcss": {
        "plugins": {
            "tailwindcss": {},
            "autoprefixer": {}
        }
    }
}
