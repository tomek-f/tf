{
    "name": "@tf/next-playground",
    "version": "1.1.0",
    "private": true,
    "type": "module",
    "engines": {
        "node": "^18 || ^20"
    },
    "scripts": {
        "dev": "rimraf ./.next && next dev",
        "dev-turbo": "rimraf ./.next && next dev --turbo",
        "build": "rimraf ./.next && next build",
        "build-and-preview": "npm run build && npm run preview",
        "preview": "next start",
        "start": "next start",
        "lint-js-next": "next lint",
        "lint-js": "NODE_ENV=production eslint --ext .js,.jsx,.cjs,.mjs,.ts,.tsx ./",
        "lint-js-fix": "npm run lint-js --fix",
        "check-types": "tsc --skipLibCheck true -p ./tsconfig.json && echo 'no problems'",
        "next": "next"
    },
    "dependencies": {
        "@hookform/resolvers": "^3.3.2",
        "@reduxjs/toolkit": "^1.9.7",
        "clsx": "^2.0.0",
        "lucide-react": "^0.292.0",
        "next": "14.0.1",
        "react": "^18.2.0",
        "react-dom": "^18.2.0",
        "react-hook-form": "^7.47.0",
        "zod": "^3.22.4"
    },
    "devDependencies": {
        "@types/node": "^20.8.10",
        "@types/react": "^18.2.34",
        "@types/react-dom": "^18.2.14",
        "autoprefixer": "^10.4.16",
        "eslint": "^8.53.0",
        "eslint-config-next": "14.0.1",
        "postcss": "^8.4.31",
        "tailwindcss": "^3.3.5",
        "typescript": "^5.2.2"
    },
    "postcss": {
        "plugins": {
            "tailwindcss": {},
            "autoprefixer": {}
        }
    }
}
